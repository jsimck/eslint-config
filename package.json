{
  "name": "@jsimck/eslint-config",
  "version": "3.0.0-next.2",
  "description": "Very opinionated eslint config I use on my projects, includes: prettier, TS, vitest, sonarqube and unicorn. And many others.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsimck/eslint-config"
  },
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./configs": {
      "types": "./src/configs/index.d.ts",
      "default": "./src/configs/index.js"
    }
  },
  "scripts": {
    "release": "changeset publish",
    "release:next": "changeset publish",
    "changeset": "changeset add",
    "changeset:enter:next": "changeset pre enter next",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix"
  },
  "peerDependencies": {
    "eslint": "9"
  },
  "dependencies": {
    "@eslint/core": "^1.1.0",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@vitest/eslint-plugin": "^1.6.7",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-sonarjs": "^3.0.7",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.55.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "eslint": "^9.39.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}